package arvore;

public class Principal {

	public static void main(String[] args) {
		No noRaiz = new No(34);
		
		ControleArvore controleArvore = new ControleArvore();
		controleArvore.adicionar(noRaiz, 14);
		controleArvore.adicionar(noRaiz, 27);
		controleArvore.adicionar(noRaiz, 22);
		controleArvore.adicionar(noRaiz, 17);
		controleArvore.adicionar(noRaiz, 5);
		controleArvore.adicionar(noRaiz, 3);
		controleArvore.adicionar(noRaiz, 25);
		System.out.println("Acabou...");
		
		controleArvore.prefixo(noRaiz);
		System.out.println("Acabou...");
		controleArvore.posordem(noRaiz);
	}

}
